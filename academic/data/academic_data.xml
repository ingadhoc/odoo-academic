<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="division_analysis_chart" model="academic.division_analysis">
        <field name="name">Division Analysis</field>

        <field name="subi_internal_lang_ind_id" search="[('name','ilike','Promedio notas trimestrales (interno)')]"/>
        <field name="subi_internal_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_internal_lang_weight" eval="15"/>

        <field name="subi_internal_math_ind_id" search="[('name','ilike','Promedio notas trimestrales (interno)')]"/>
        <field name="subi_internal_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_internal_math_weight" eval="15"/>

        <field name="subi_external_lang_ind_id" search="[('name','ilike','Promedio Evaluación Externa')]"/>
        <field name="subi_external_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_external_lang_weight" eval="35"/>

        <field name="subi_external_math_ind_id" search="[('name','ilike','Promedio Evaluación Externa')]"/>
        <field name="subi_external_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_external_math_weight" eval="35"/>


        <field name="subi_avg_hour_lang_ind_id" search="[('name','ilike','Porcentaje de horas dadas')]"/>
        <field name="subi_avg_hour_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_avg_hour_lang_weight" eval="15"/>

        <field name="subi_avg_hour_math_ind_id" search="[('name','ilike','Porcentaje de horas dadas')]"/>
        <field name="subi_avg_hour_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_avg_hour_math_weight" eval="15"/>


        <field name="subi_avg_topics_lang_ind_id" search="[('name','ilike','Porcentaje de temas desarrollados')]"/>
        <field name="subi_avg_topics_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_avg_topics_lang_weight" eval="15"/>

        <field name="subi_avg_topics_math_ind_id" search="[('name','ilike','Porcentaje de temas desarrollados')]"/>
        <field name="subi_avg_topics_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_avg_topics_math_weight" eval="15"/>


        <field name="subi_avg_spec_proj_lang_ind_id" search="[('name','ilike','Proyectos específicos')]"/>
        <field name="subi_avg_spec_proj_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_avg_spec_proj_lang_weight" eval="10"/>

        <field name="subi_avg_spec_proj_math_ind_id" search="[('name','ilike','Proyectos específicos')]"/>
        <field name="subi_avg_spec_proj_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_avg_spec_proj_math_weight" eval="10"/>


        <field name="subi_avg_relevant_training_lang_ind_id" search="[('name','ilike','Capacitación pertinente')]"/>
        <field name="subi_avg_relevant_training_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_avg_relevant_training_lang_weight" eval="10"/>

        <field name="subi_avg_relevant_training_math_ind_id" search="[('name','ilike','Capacitación pertinente')]"/>
        <field name="subi_avg_relevant_training_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_avg_relevant_training_math_weight" eval="10"/>


        <field name="subi_avg_teacher_interview_lang_ind_id" search="[('name','ilike','Cantidad de entrevistas con docentes')]"/>
        <field name="subi_avg_teacher_interview_lang_sub_id" search="[('name','like','Lengua')]"/>
        <field name="subi_avg_teacher_interview_lang_weight" eval="30"/>

        <field name="subi_avg_teacher_interview_math_ind_id" search="[('name','ilike','Cantidad de entrevistas con docentes')]"/>
        <field name="subi_avg_teacher_interview_math_sub_id" search="[('name','like','Matemática')]"/>
        <field name="subi_avg_teacher_interview_math_weight" eval="30"/>

    </record>

    <record forcecreate="True" id="decimal_sub_indicator_value" model="decimal.precision">
        <field name="name">Sub Indicator Value</field>
        <field name="digits">0</field>
    </record>
    <record forcecreate="True" id="decimal_sub_indicator_weight" model="decimal.precision">
        <field name="name">Sub Indicator Weight</field>
        <field name="digits">0</field>
    </record>

</odoo>
